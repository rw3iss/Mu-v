{
  "name": "preact-basic",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "dev": "cross-env NODE_PATH=./src NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "app": "cd build_electron && NODE_PATH=./ && electron .",

    "clean": "rm -rf ./dist",
    "clean:osx": "rm -rf ./dist/osx",
    "clean:win": "rm -rf ./dist/win",

    "pack": "npm run clean && npm run pack:osx && npm run pack:win",
    "pack:osx": "npm run clean:osx && electron-packager . \"Movie\" --out=dist/osx --platform=darwin --arch=x64 --version=0.36.2 --ignore=dist --ignore=assets --ignore=build --ignore=build_electron --ignore=src --ignore=builder.json --ignore=bower.json --ignore=README.md --ignore=.gitignore",
    "pack:win": "npm run clean:win && electron-packager . \"Movie\" --out=dist/win --platform=win32 --arch=ia32 --version=0.36.2 --icon=assets/win/logo.ico --ignore=dist --ignore=assets --ignore=builder.json --ignore=bower.json --ignore=README.md --ignore=.gitignore --ignore=preview.png",

    "build": "npm run build:osx && npm run build:win",
    "build:osx": "npm run pack:osx && electron-builder \"dist/osx/Movies-darwin-x64/Movies.app\" --platform=osx --out=\"dist/osx\" --config=builder.json",
    "build:win": "npm run pack:win && electron-builder \"dist/win/EA Todo-win32-ia32\" --platform=win --out=\"dist/win\" --config=builder.json"
  },
  "keywords": [
    "preact",
    "boilerplate",
    "webpack"
  ],
  "build": {
      "appId": "Movies.id",
      "mac": {
        "category": "entertainment.media"
    }
  },
   "files": [
      "**/*",
      "build_electron/*"
    ],
  "main": "main.js",
  "license": "MIT",
  "author": "Ryan Weiss <rw3iss@gmail.com>",
  "description": "App to organize local movie library and search for new movies",
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel": "^6.5.2",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "electron": "^1.6.1",
    "electron-builder": "^15.1.1",
    "electron-packager": "^8.5.2",
    "electron-reload": "^1.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "fs": "0.0.1-security",
    "fs-readdir-promise": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "install": "^0.8.7",
    "isparta-loader": "^2.0.0",
    "js-utils": "file:src/lib/util/js-utils",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "mime-types": "^2.1.14",
    "mkdirp": "^0.5.1",
    "mmmagic": "^0.4.5",
    "moviedb": "^0.2.7",
    "ncp": "^2.0.0",
    "node-sass": "^4.3.0",
    "npm": "^4.3.0",
    "object-hash": "^1.1.7",
    "offline-plugin": "^4.5.3",
    "postcss-loader": "^1.2.1",
    "raw-loader": "^0.5.1",
    "rc-animate": "^2.3.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "replace-bundle-webpack-plugin": "^1.0.0",
    "sass-loader": "^4.1.1",
    "script-ext-html-webpack-plugin": "^1.3.4",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "v8-lazy-parse-webpack-plugin": "^0.3.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0",
    "webpack-sources": "^0.1.4"
  },
  "dependencies": {
    "eventbusjs": "^0.1.0",
    "is-video": "^1.0.1",
    "preact": "^6.0.0",
    "preact-compat": "^3.0.0",
    "preact-router": "^2.0.0",
    "promise-polyfill": "^6.0.2",
    "proptypes": "^0.14.3",
    "react-router": "^3.0.2",
    "serve": "^2.0.0"
  }
}
